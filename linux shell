# BBTool qulity control
bbmap/bbduk.sh maxns=1 maq=20 trimq=20 qtrim=rl ktrim=r ordered minlen=100 minlenfraction=0.33 tbo=t tpe=t rcomp=f k=23 ow=t zl=4 in={Input_filename} out={output_filename} ref=bbmap/resources/adapters.fa


# multiplex fastq
for i in `ls -1 *_1.fastq.gz | sed 's/_1.fastq.gz//'`; do ~/Analysis/tool/bbmap/bbduk.sh maxns=1 maq=20 trimq=20 qtrim=rl ktrim=r ordered minlen=100 minlenfraction=0.33 tbo=t tpe=t rcomp=f k=23 ow=t zl=4 in1=$i\_1.fastq.gz in2=$i\_2.fastq.gz out1=$i\QC_1.fastq.gz out2=$i\QC_2.fastq.gz ref=~/Analysis/tool/bbmap/resources/adapters.fa;done
